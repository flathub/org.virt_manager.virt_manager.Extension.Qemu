{
    "description": "x64 UEFI for x86_64, 4MB FD",
    "interface-types": [
        "uefi"
    ],
    "mapping": {
        "device": "flash",
        "executable": {
            "filename": "/app/lib/extensions/share/ovmf/x64/OVMF_CODE.4m.fd",
            "format": "raw"
        },
        "nvram-template": {
            "filename": "/app/lib/extensions/share/ovmf/x64/OVMF_VARS.4m.fd",
            "format": "raw"
        }
    },
    "targets": [
        {
            "architecture": "x86_64",
            "machines": [
                "pc-i440fx-*",
                "pc-q35-*"
            ]
        }
    ],
    "features": [
        "acpi-s3",
        "acpi-s4",
        "amd-sev",
        "verbose-dynamic"
    ],
    "tags": [

    ]
}

